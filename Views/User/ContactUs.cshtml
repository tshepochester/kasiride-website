@model KASY_RIDES.Models.Rides

@{
    ViewData["Title"] = "ContactUs";
}

<style>
    body {
        font-family: Arial, sans-serif;
        margin: 0;
        padding: 0;
        background-color: #f8f8f8;
    }

    .container {
        max-width: 800px;
        margin: 40px auto;
        padding: 20px;
        /* background: white; */
        background-image: url('https://t3.ftcdn.net/jpg/04/99/56/30/240_F_499563059_GGQxzylCFmlUlL1jr5rVrbYE9yk9vkmp.jpg');
        background-size: cover;
        border: 1px solid#ddd;
        border-radius: 10px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    }

    header {
        background-color: #000;
        color: #fff;
        padding: 20px;
        text-align: center;
    }



    h1, h2, h3 {
        
        margin-bottom: 10px;
    }

    nav ul {
        list-style: none;
        padding: 0;
    }

        nav ul li {
            display: inline;
            margin: 0 15px;
        }

            nav ul li a {
                color: white;
                text-decoration: none;
            }

    main {
        padding: 20px;
    }



    .contact-info {
        background: white;
        padding: 20px;
        margin: 15px 0;
        border-radius: 5px;
        box-shadow: 0 2px 5px rgba(0,0,0,0.1);
    }

    .contact-form {
        background: white;
        padding: 20px;
        margin: 15px 0;
        border-radius: 5px;
        box-shadow: 0 2px 5px rgba(0,0,0,0.1);
    }
/* 
    label {
        display: block;
        margin: 10px 0 5px;
    } */


    footer {
        background-color: #333;
        color: #fff;
        padding: 1em;
        text-align: center;
        clear: both;
    }

</style>

<body>
    <header>
        <h1>Contact Us</h1>
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS02qPnvJZZtWoCfpC1S7quJUEsSJxehJQmOQ&s" alt="logo" width="100" height="100" />
    </header>
    <div class="container">
    <main>
        <section class="contact-info">
