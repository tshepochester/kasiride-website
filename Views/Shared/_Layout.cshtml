@using BackendlessAPI;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Kasy_Rides</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Kasy_Rides.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Kasy_Rides</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">                        
                        @{
                            if(Backendless.UserService.IsValidLogin() == false)
                            {
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Login">Login</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Register">Register</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Logout">Logout</a>
                                </li>
                                                      }
                            else
                            {
                                if(Backendless.UserService.CurrentUser.GetProperty("Role").ToString() == "Admin")
                                {
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="AboutUs">About Us</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="ContactUs">Contact Us</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Profile">Profile</a>
                                    </li>
                                    // <li class="nav-item">
                                    //     <a class="nav-link text-dark" asp-area="Admin" asp-controller="Admin" asp-action="Index">AdminDashboard</a>
                                    // </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="Users">Users</a>
                                    // </li>
                                    // <li class="nav-item">
                                    //     <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Profile">Profile</a>
                                    // </li>                                   
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="FAQ">FAQ</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Driver" asp-action="Rides">Rides</a>
                                    </li>
                                    
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="TaxiBank" asp-action="CreateAccount">SH Bank</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="BankAccounts">Bank Accounts</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="PassengerPayments">Passenger Payments</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Admin" asp-action="DriverPayments">Driver Payments</a>
                                    </li>
                                    
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Logout">Logout</a>
                                    </li>
                                }
                                else if(Backendless.UserService.CurrentUser.GetProperty("Role").ToString() == "Driver")
                                {
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="AboutUs">About Us</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="ContactUs">Contact Us</a>
                                    </li>
                                    // <li class="nav-item">
                                    //     <a class="nav-link text-dark" asp-area="Driver" asp-controller="Driver" asp-action="Index">DriverDashboard</a>
                                    // </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Profile">Profile</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="TaxiBank" asp-action="CreateAccount">SH Bank</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Driver" asp-action="Rides">Rides</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Driver" asp-action="MyRides">MyRides</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Driver" asp-action="MyPayments">My Payments</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="FAQ">FAQ</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Logout">Logout</a>
                                    </li>
                                }
                                else
                                {
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="AboutUs">About Us</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="ContactUs">Contact Us</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Profile">Profile</a>
                                    </li>
                                    // <li class="nav-item">
                                    //     <a class="nav-link text-dark" asp-area="Passenger" asp-controller="Passenger" asp-action="Index">PassengerDashboard</a>
                                    // </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Passenger" asp-action="BookRide">BookRide</a>
                                    </li>
                                   
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="TaxiBank" asp-action="CreateAccount">SH Bank</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Passenger" asp-action="MyRides">MyRides</a>
                                    </li>                                    
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Passenger" asp-action="MyPayments">MyPayments</a>
                                    </li>
                                   
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="FAQ">FAQ</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Logout">Logout</a>
                                    </li>
                                }
                            }
                        }                     
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Kasy_Rides - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
        <div class="center">
            <a href="https://web.whatsapp.com/" target="Whatsapp" style="margin-right: 60px;">
                <img src="data:image/jpeg;base64," alt="WhatsApp" style="width: 40px; height: 40px;">
                Whatsapp
            </a>
            <a href="https://www.facebook.com/" target="Facebook" style="margin-right: 60px;">
                <img src="data:image/png;base64," alt="Facebook" style="width: 40px; height: 40px;">
                Facebook
            </a>
            <a href="https://www.instagram.com/accounts/login/" target="Instagram" style="margin-right: 60px;">
                <img src="data:image/jpeg;base64," alt="Instagram" style="width: 40px; height: 40px;">
                Instagram
            </a>
            <a href="https://www.linkedin.com/login" target="Linkedin" style="margin-right: 60px;">
                <img src="data:image/jpeg;base64," alt="Linkedin" style="width: 40px; height: 40px;">
                LinkedIn
            </a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
